{
  "ClassType":  {
    "_enum": {
      "Null": null,
      "X1":"ProgramType",
      "X2":"(ProgramType, ProgramType)",
      "X3":"(ProgramType, ProgramType, ProgramType)",
      "X4":"(ProgramType, ProgramType, ProgramType, ProgramType)",
      "X5":"(ProgramType, ProgramType, ProgramType, ProgramType, ProgramType)",
      "X6":"(ProgramType, ProgramType, ProgramType, ProgramType, ProgramType, ProgramType)"
    }
  },
  "Index": "u32",
  "ProgramOption":  {
    "_enum": {
      "Null": null,
      "Index":"Index",
      "Range":"Range",
      "Other": "Vec<u8>"
    }
  },
  "Range":  {
    "_enum": {
      "LargeThan": "u32",
      "SmallerThan":"u32",
      "Between":"(u32, u32)"
    }
  },
  "ProgramType":  {
    "_enum": {
      "Null": null,
      "Age":"ProgramOption",
      "Country":"ProgramOption",
      "Other": "Vec<u8>"
    }
  },
  "TaskStatus": {
    "_enum": [
      "JustCreated",
      "Verifying",
      "VerifiedTrue",
      "VerifiedFalse"
    ]
  },
  "UserTaskStatus": {
    "_enum": [
      "JustCreated",
      "VerifiedTrue",
      "VerifiedFalse"
    ]
  },
  "CrowdfundingOption": {
    "_enum": [
      "CreateAsset",
      "TransferAsset"
    ]
  },
  "Status": {
    "verifiers": "Vec<u32>",
    "ayes": "u32",
    "nays": "u32"
  },
  "VerificationReceipt": {
    "program_hash": "[u8; 32]",
    "passed": "bool",
    "submit_at": "BlockNumber",
    "auth_index": "u32",
    "validator_len": "u32"
  },
  "TaskInfo": {
    "proof_id": "Vec<u8>",
    "inputs": "Vec<u128>",
    "outputs": "Vec<u128>",
    "program_hash": "[u8; 32]",
    "is_task_finish": "Option<TaskStatus>",
    "expiration": "Option<BlockNumber>"
  },

  "AssetId": "u32",
  "LookupSource": "MultiAddress",
  "TAssetBalance": "u128",
  "CrowfundingStatus": {
    "admin": "Option<AccountId>",
    "funding_account": "Option<AccountId>",
    "funding_begin": "BlockNumber",
    "funding_expiration": "BlockNumber",
    "total_funding": "Balance",
    "remain_funding": "Balance",
    "is_funding_proceed": "Option<bool>",
    "ratio": "Balance"
  },
  "SeperateStatus": {
    "verifiers": "Vec<u32>",
    "ayes": "u32",
    "nays": "u32",
    "come_first": "Option<bool>"
  },
  "VerifyClass": {
    "_enum": {
      "Age":"u32",
      "Country":"u32"
  }
  },
  "KYCListOption": {
    "_enum": {
      "Add": "ADD",
      "Delete": "DEL"
    }
  },
  "ADD": {
    "kyc_verify_class": "ClassType",
    "program_hash": "[u8; 32]]"
  },
  "DEL": {
    "kyc_verify_class": "ClassType"
  }
